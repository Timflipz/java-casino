         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Label>


      <Button fx:id="hogerLagerButton" layoutX="46.0" layoutY="112.0" mnemonicParsing="false" onAction="#hogerLagerOpenen" prefHeight="151.0" prefWidth="261.0" text="Hoger Lager" textFill="#96881b">


      <Button fx:id="hogerLagerButton" layoutX="29" layoutY="98.0" mnemonicParsing="false" onAction="#hogerLagerOpenen" prefHeight="102.0" prefWidth="261.0" text="Hoger Lager" textFill="#db00ff">

         <font>
            <Font name="System Bold" size="48.0" />
         </font>
      </Button>

      <Button fx:id="blackJackButton" layoutX="324.0" layoutY="112.0" mnemonicParsing="false" onAction="#blackJackOpenen" prefHeight="151.0" prefWidth="261.0" text="Black Jack" textFill="#96881b">

