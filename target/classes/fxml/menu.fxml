         <font>
            <Font name="System Bold Italic" size="48.0" />
         </font>
      </Label>
      <Button fx:id="geldActies" layoutX="437.0" layoutY="27.0" mnemonicParsing="false" onAction="#update" text="Geld innen/toevoegen" />

      <Button layoutX="46.0" layoutY="278.0" mnemonicParsing="false" prefHeight="151.0" prefWidth="261.0" text="Slots" textFill="#96881b">
