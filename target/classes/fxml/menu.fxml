<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="555.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taak.grotetaakcasi.SpeelbordController">
