<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taak.grotetaakcasi.TafelController">
   <children>
      <TextField fx:id="budgetIngeven" layoutX="95.0" layoutY="91.0" onKeyPressed="#update" prefHeight="31.0" prefWidth="409.0" promptText="Geef uw bedrag in">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="voegToe" defaultButton="true" layoutX="95.0" layoutY="149.0" mnemonicParsing="false" onAction="#GeldToevoegen" prefHeight="75.0" prefWidth="409.0" text="GELD STORTEN" textAlignment="JUSTIFY">
         <font>
            <Font name="System Bold" size="36.0" />
         </font></Button>
      <Button fx:id="menuKnop" layoutX="245.0" layoutY="342.0" mnemonicParsing="false" onAction="#gaNaarMenu" prefHeight="39.0" prefWidth="109.0" text="MENU">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Label fx:id="budgetLabel" layoutX="38.0" layoutY="29.0" text="Budget: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="inGeld" defaultButton="true" layoutX="95.0" layoutY="246.0" mnemonicParsing="false" onAction="#geldInnen" prefHeight="77.0" prefWidth="409.0" text="Binnenharken">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Button>
      <Label layoutX="200.0" layoutY="56.0" text="Geef uw bedrag in:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
